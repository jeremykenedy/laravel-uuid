{
	"name" : "jeremykenedy/uuid",
	"description" : "Laravel package to generate a UUID according to the RFC 4122 standard. UUID Versions 1, 3, 4 and 5 are supported. With MIT license.",
	"authors" : [
        {
			"name" : "Christoph Kempen",
			"email" : "christoph@downsized.nl"
		},
        {
            "name" : "Jeremy Kenedy",
            "email" : "jeremykenedy@gmail.com"
        }
	],
	"keywords" : [
		"UUID RFC4122"
	],
	"homepage" : "https://github.com/jeremykenedy/laravel-uuid",
	"license" : [
		"MIT"
	],
	"require" : {
		"php" : ">=5.3.0"
	},
    "require-dev" : {
      "phpunit/phpunit": "4.7.*",
      "fzaninotto/faker": "1.5.*"
    },
	"autoload" : {
		"psr-0" : {
			"jeremykenedy\\Uuid" : "src/"
		}
	},
	"support" : {
		"source" : "https://github.com/jeremykenedy/laravel-uuid",
		"issues" : "https://github.com/jeremykenedy/laravel-uuid/issues"
	},
	"suggest" : {
		"paragonie/random_compat": "A random_bytes Php 5.x polyfill."
	},
    "extra": {
        "laravel": {
            "aliases": {
                "Uuid": "jeremykenedy\\Uuid\\UuidFacade"
            }
        }
    }
}
